<h2>Highscore</h2>

<table class="table">
  <tr>
    <th>Word</th>
    <th>Score</th>
  </tr>

  <% @highscore.each do |result| %>
    <tr><td><%= result["answer"] %></td><td><%= result["score"] %></td></tr>
  <% end %>

</table>
<br>
  <%= link_to "Clear Highscore", clear_path , class: "btn btn-primary", method: :delete%>

